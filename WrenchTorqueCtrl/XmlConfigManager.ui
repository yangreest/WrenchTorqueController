         <widget class="QTextEdit" name="textEdit">
          <property name="styleSheet">
           <string notr="true">/* ========== 新增/优化：QTextEdit 样式 ========== */
QTextEdit {
    background-color: #ffffff;
    border: 1px solid #d0d6e0;
    border-radius: 4px;
    padding: 8px 10px;
    color: #333842;
    font-size: 12px;
    line-height: 1.4;
    outline: none; /* 去除聚焦虚线框 */
}

/* 文本框悬浮效果 */
QTextEdit:hover {
    border-color: #a0b3dc;
}

/* 文本框聚焦效果 */
QTextEdit:focus {
    border-color: #1e6ceb;
    box-shadow: 0 0 0 2px rgba(30, 108, 235, 0.1); /* 柔和的聚焦阴影 */
}

/* 只读状态的文本框（如需） */
QTextEdit[readOnly=&quot;true&quot;] {
    background-color: #f8f9fc;
    color: #555a64;
    border-color: #e5e9f2;
}</string>
